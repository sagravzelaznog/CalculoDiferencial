# Variables de Entorno para el Curso de Cálculo Diferencial

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Application Configuration
REACT_APP_APP_NAME=Curso de Cálculo Diferencial
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-domain.com/api
REACT_APP_API_TIMEOUT=30000

# Authentication Configuration
REACT_APP_AUTH_DOMAIN_WHITELIST=estudiante.edu.mx,profesor.edu.mx,admin.edu.mx
REACT_APP_SESSION_TIMEOUT=3600000

# Analytics Configuration
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=false
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_DARK_MODE=true

# Math Rendering Configuration
REACT_APP_MATH_RENDERER=katex
REACT_APP_MATH_DELIMITERS=latex

# Course Configuration
REACT_APP_COURSE_NAME=Cálculo Diferencial
REACT_APP_COURSE_VERSION=1.0
REACT_APP_MAX_MODULES=5
REACT_APP_DEFAULT_LANGUAGE=es

# Security Configuration
REACT_APP_CSP_NONCE=your_nonce_here
REACT_APP_ENABLE_CSP=true

# Development Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_HOT_RELOAD=true
